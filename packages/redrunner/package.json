{
  "name": "redrunner",
  "version": "0.2.0",
  "description": "A tiny framework with legs",
  "scripts": {
    "prepublish": "npm test && babel src -d lib",
    "build": "NODE_ENV=production babel src -d lib",
    "test": "jest --clearCache && jest --coverage",
    "clear-jest": "jest --clearCache"
  },
  "author": "Andrew Buchan",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "babel-plugin-redrunner",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "files": [
    "lib",
    "src"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyhasit/redrunner"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "html": "^1.0.0",
    "jest": "^26.0.1",
    "microbundle": "^0.12.0"
  },
  "dependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-redrunner": "^0.2.0"
  }
}
