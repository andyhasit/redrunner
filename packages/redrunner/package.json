{
  "name": "redrunner",
  "version": "0.9.0",
  "description": "A tiny framework with legs",
  "scripts": {
    "prepublish": "npm test && npm run build",
    "build": "NODE_ENV=production rollup -c",
    "test": "jest --clearCache && jest",
    "clear-jest": "jest --clearCache"
  },
  "author": "Andrew Buchan",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "babel-plugin-redrunner",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "main": "dist/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyhasit/redrunner"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-babel": "^5.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "html": "^1.0.0",
    "jest": "^26.0.1"
  },
  "dependencies": {
    "babel-plugin-redrunner": "^0.9.0"
  },
  "gitHead": "d0b7e57549514d9141b4bcf3109269c9ad43a484"
}
