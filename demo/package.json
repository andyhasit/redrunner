{
  "name": "redrunner-demo",
  "version": "0.0.1",
  "description": "RedRunner demo",
  "scripts": {
    "build": "npm run copy-both && webpack -p",
    "dev": "webpack -w -d",
    "copy-redrunner-dist": "cp ../packages/redrunner/dist/bundle.js node_modules/redrunner/dist",
    "copy-redrunner-src": "cp -r ../packages/redrunner/src/* node_modules/redrunner/src",
    "copy-router-dist": "cp ../packages/redrunner-router/dist/bundle.js node_modules/redrunner-router/dist",
    "copy-router-src": "cp -r ../packages/redrunner-router/src/* node_modules/redrunner-router/src",
    "copy-plugin": "cp -r ../packages/babel-plugin-redrunner/lib/* node_modules/babel-plugin-redrunner/lib",
    "copy-all": "npm run copy-redrunner-src && npm run copy-redrunner-dist && npm run copy-router-src && npm run copy-router-dist",
    "start": "webpack-dev-server --open",
    "watch": "webpack --watch"
  },
  "author": "Andrew Buchan",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-redrunner": "0.2.0",
    "redrunner": "0.2.0",
    "redrunner-router": "^0.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
